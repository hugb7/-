#!url=https://raw.githubusercontent.com/hugb7/-/refs/heads/main/Shadowrocket/模块/解锁Spotify.module
#!name=解锁Spotify
#!desc=去除播放广告。歌手/专辑列表正常展示。去除随机播放。注意：音质不能设置为超高。

[Script]
# 功能: 1.去除播放广告 2.歌手/专辑列表正常展示 3.去除随机播放。注意：音质不能设置为超高
spotify-qx-header = type=http-request, pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/user-customization-service\/v1\/customize$, script-path=https://raw.githubusercontent.com/hugb7/-/refs/heads/main/Shadowrocket/js/spotify-qx-header.js, timeout=60
spotify-proto = type=http-response, pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$, script-path=https://raw.githubusercontent.com/hugb7/-/refs/heads/main/Shadowrocket/js/spotify-proto.js, requires-body=true, binary-body-mode=true, max-size=-1, timeout=60
spotify-json = type=http-request, pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/(artistview\/v1\/artist|album-entity-view\/v2\/album)\/, script-path=https://raw.githubusercontent.com/hugb7/-/refs/heads/main/Shadowrocket/js/spotify-json.js, timeout=60

[MITM]
hostname = spclient.wg.spotify.com, *spclient.spotify.com